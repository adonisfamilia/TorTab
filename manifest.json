{
  "manifest_version": 2,
  "name": "TorTab",
  "description": "It's a Tor tab.",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
    }
  ],
  "background": {
    "scripts": ["tortab.js"]
  },
  "permissions": [
      "proxy",
      "contextualIdentities",
      "cookies",
      "storage",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "tabs"
  ]
}
